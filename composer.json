{
    "name": "aihealth/laravel-monitor",
    "description": "AI Store Health Monitor SDK for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AIHealth\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AIHealth\\Laravel\\AIHealthServiceProvider"
            ],
            "aliases": {
                "AIHealth": "AIHealth\\Laravel\\Facades\\AIHealth"
            }
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
    }
}